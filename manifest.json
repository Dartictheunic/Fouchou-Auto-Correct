{
"manifest_version": 2,
    "name": "Fouchou Auto Correct",
    "description": "Ta gueule fouchou",
    "version": "1.0",
  "icons": {
    "128": "128.png"
  },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}